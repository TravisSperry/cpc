%p#notice= notice
%h1 Work Order Schedules
%table
  %thead
    %tr
      %th Work order
      %th{:colspan => "3"}
  %tbody
    - @work_order_schedules.each do |work_order_schedule|
      %tr
        %td= work_order_schedule.work_order
        %td= link_to 'Show', work_order_schedule
        %td= link_to 'Edit', edit_work_order_schedule_path(work_order_schedule)
        %td= link_to 'Destroy', work_order_schedule, method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New Work Order Schedule', new_work_order_schedule_path
