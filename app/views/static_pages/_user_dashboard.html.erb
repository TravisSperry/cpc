<div class="container">
  <div id="page-wrapper">

    <div class="row-fluid">
      <div class="col-lg-12">
        <h1>Dashboard <small>Statistics Overview</small></h1>
        <ol class="breadcrumb">
          <li class="active"><i class="icon-dashboard"></i> Dashboard</li>
        </ol>
      </div>
    </div><!-- /.row -->

    <div class="row-fluid">

      <div class="col-lg-4  ">
        <div class="panel panel-success">
          <div class="panel-heading">
            <div class="row">
              <div class="col-xs-6">
                <i class="icon icon-list icon-5x"></i>
              </div>
              <div class="col-xs-6 text-right">
                <p class="announcement-heading"><%= @new_quote_requests.count ? @new_quote_requests.count : 0 %></p>
                <p class="announcement-text">New Quote <%= pluralize_without_count(@new_quote_requests.count, 'Request')%><%= @new_quote_requests.count == 0 ? "" : "!" %></p>
              </div>
            </div>
          </div>
          <a href="<%= quote_requests_path %>">
            <div class="panel-footer announcement-bottom">
              <div class="row">
                <div class="col-xs-8">
                  View Quote Requests
                </div>
                <div class="col-xs-6 text-right">
                  <i class="icon icon-angle-down"></i>
                </div>
              </div>
            </div>
          </a>
        </div>
      </div>
    </div><!-- /.row -->

    <div class="row">
      <div class="col-lg-4">
        <div class="panel panel-primary">
          <div class="panel-heading">
            <h3 class="panel-title"><i class="fa fa-clock-o"></i> Recent Activity</h3>
          </div>
          <div class="panel-body">
            <div class="list-group">
              <%= render_activities(@activities, layout: :activity) %>
            </div>
            <div class="text-right">
              <a href="#">View All Activity <i class="fa fa-arrow-circle-right"></i></a>
            </div>
          </div>
        </div>
      </div>

    </div><!-- /.row -->
  </div><!-- /#page-wrapper -->
