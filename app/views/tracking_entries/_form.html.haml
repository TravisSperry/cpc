= form_for @tracking_entry do |f|
  - if @tracking_entry.errors.any?
    #error_explanation
      %h2= "#{pluralize(@tracking_entry.errors.count, "error")} prohibited this tracking_entry from being saved:"
      %ul
        - @tracking_entry.errors.full_messages.each do |message|
          %li= message

  .field
    = f.label :start_time
    = f.datetime_select :start_time
  .field
    = f.label :end_time
    = f.datetime_select :end_time
  .field
    = f.label :deleted_at
    = f.datetime_select :deleted_at
  .field
    = f.label :service
    = f.text_field :service
  .actions
    = f.submit 'Save'
