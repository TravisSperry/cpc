<h1>Listing quote_request_attachments</h1>

<table>
  <thead>
    <tr>
      <th>Quote request</th>
      <th>Attachment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quote_request_attachments.each do |quote_request_attachment| %>
      <tr>
        <td><%= quote_request_attachment.quote_request_id %></td>
        <td><%= quote_request_attachment.attachment %></td>
        <td><%= link_to 'Show', quote_request_attachment %></td>
        <td><%= link_to 'Edit', edit_quote_request_attachment_path(quote_request_attachment) %></td>
        <td><%= link_to 'Destroy', quote_request_attachment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Quote request attachment', new_quote_request_attachment_path %>
