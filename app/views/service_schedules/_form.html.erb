<%= simple_form_for(@service_schedule) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :work_order_schedule %>
    <%= f.association :service %>
    <%= f.input :start_date %>
    <%= f.input :end_date %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
